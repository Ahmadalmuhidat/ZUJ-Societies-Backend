Table: Comments
Columns:
ID varchar(200) PK 
Content text 
Post varchar(200) 
User varchar(200)

Table: Events
Columns:
ID varchar(200) PK 
Title varchar(200) 
Description text 
Date date 
Time time 
User varchar(200) 
Society varchar(200)

Table: Posts
Columns:
ID varchar(200) PK 
Content text 
Likes int 
Comments int 
User varchar(200)

Table: Societies
Columns:
ID varchar(200) PK 
Name varchar(200) 
Description varchar(200) 
User varchar(200)

Table: Societies_Join_Requests
Columns:
ID varchar(200) 
Society varchar(200) 
User varchar(200) 
Status varchar(200)

Table: Societies_Members
Columns:
ID varchar(200) 
Society varchar(200) 
User varchar(200) 
Role varchar(200)

Table: Users
Columns:
ID varchar(200) PK 
Name varchar(200) 
Email varchar(200) 
Password varchar(200) 
Student_ID varchar(200) 
Photo varchar(200)